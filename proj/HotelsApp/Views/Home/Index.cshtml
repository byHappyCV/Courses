@model IEnumerable<HotelsApp.Models.MeetingRoom>
@{
    ViewBag.Title = "Home Page";
}



<div class="row">
    <table class="table table-striped">
            <tr>
                <th>Id</th>
                <th>FName</th>
                <th>MName</th>
            </tr>

            <tbody>
            @foreach (var item in Model)
            {
                <tr onclick='location.href = "@Url.Action("ShowInfo", "Home", new {id = item.Id})"'>
                    <td>@item.Id</td>
                    <td>@item.Location</td>
                    <td>@item.Title</td>
                </tr>
            }
            <tr></tr>
            </tbody>


        </table>
</div>