@model IEnumerable<testapp.Models.MeetingRooms>
@{
    ViewBag.Title = "ShowRooms";
}

<h2>ShowRooms</h2>

<div class="row">
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>FName</th>
            <th>MName</th>
        </tr>

        <tbody>
            @foreach (var item in Model)
            {
                <tr onclick='location.href = "@Url.Action("ShowInfo", "Home", new {id = item.Id})"'>
                    <td>@item.Id</td>
                    <td>@item.Location</td>
                    <td>@item.Title</td>
                </tr>
            }
            <tr></tr>
        </tbody>


    </table>
</div>
